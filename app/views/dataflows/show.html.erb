<%- model_class = Dataflow -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:dataset_id) %>:</strong></dt>
  <dd><%= @dataflow.dataset_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:interface_id) %>:</strong></dt>
  <dd><%= @dataflow.interface_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:frequency_id) %>:</strong></dt>
  <dd><%= @dataflow.frequency_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              dataflows_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_dataflow_path(@dataflow), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              dataflow_path(@dataflow),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
